SELECT * FROM province;

-- limit 
SELECT *FROM province
    limit 10;

SELECT *FROM province 
    ORDER BY area_km2 DESC 
    LIMIT 10;

--fetch
SELECT *FROM province 
    ORDER BY area_km2 DESC 
    FETCH FIRST 10  ROW ONLY;

SELECT *FROM province 
    ORDER BY area_km2 DESC 
    OFFSET 5 ROW FETCH
    FIRST 3  ROW ONLY;

